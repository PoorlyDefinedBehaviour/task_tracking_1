{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_+" }],
    "import/prefer-default-export": "off",
    "no-template-curly-in-string": "off"
  }
}
